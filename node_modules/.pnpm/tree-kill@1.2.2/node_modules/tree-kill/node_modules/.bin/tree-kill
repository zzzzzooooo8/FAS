#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/zzzzzooooo/Desktop/青协/计算机221/毕设/FAS/node_modules/.pnpm/tree-kill@1.2.2/node_modules/tree-kill/node_modules:/mnt/c/Users/zzzzzooooo/Desktop/青协/计算机221/毕设/FAS/node_modules/.pnpm/tree-kill@1.2.2/node_modules:/mnt/c/Users/zzzzzooooo/Desktop/青协/计算机221/毕设/FAS/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/zzzzzooooo/Desktop/青协/计算机221/毕设/FAS/node_modules/.pnpm/tree-kill@1.2.2/node_modules/tree-kill/node_modules:/mnt/c/Users/zzzzzooooo/Desktop/青协/计算机221/毕设/FAS/node_modules/.pnpm/tree-kill@1.2.2/node_modules:/mnt/c/Users/zzzzzooooo/Desktop/青协/计算机221/毕设/FAS/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
